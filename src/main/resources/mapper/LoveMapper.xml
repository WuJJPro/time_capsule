<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twt.time_capsule.mapper.LoveMapper">
    <select id="getLove" resultType="com.twt.time_capsule.entity.Love">
        select * from love where uid = #{uid} and capsule_key = #{key}
    </select>

</mapper>